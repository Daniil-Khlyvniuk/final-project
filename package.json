{
  "name": "final-project",
  "version": "1.0.1",
  "engines": {
    "node": "14.x"
  },
  "description": "Final Project - Online shop \"Postil' bedding\"",
  "main": "index.js",
  "prepare": "husky install",
  "presentation": "https://docs.google.com/presentation/d/1CBFV4Z9PBZjipS-x3z4KodsS_i2IgTSlwFx6lEovX9I/edit?usp=sharing",
  "scripts": {
    "build": "npm install --prefix client && npm install --prefix server && npm run build --prefix client && rm -rf server/public && cp -a client/build/. server/public/ && rm -rf server/static/static && rm server/static/asset-manifest.json && rm server/static/index.html &&  cp -a server/public/. server/static",
    "start": "npm start --prefix server",
    "start-dev": "npm-run-all -p start:client start:server",
    "start:client": "npm start --prefix client",
    "lint:client": "npm run lint --fix --prefix client",
    "start:server": "npm run server --prefix server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Daniil-Khlyvniuk/final-project.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Daniil-Khlyvniuk/final-proejct/issues"
  },
  "homepage": "https://github.com/Daniil-Khlyvniuk/final-proejct#readme",
  "dependencies": {
    "husky": "^7.0.4",
    "npm-run-all": "^4.1.5",
    "travis-cli": "^1.0.9"
  }
}
